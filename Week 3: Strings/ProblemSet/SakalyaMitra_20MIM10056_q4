#include <bits/stdc++.h>
using namespace std;
int main(){
int T;
    cin>>T;
    while(T--)
    {
        string s,t,s1,s2;
        cin>>s>>t;
        for(size_t i=0;i<s.size();i++)
            if(s[i]!=s1[s1.size()-1])
                s1+=s[i];
        for(size_t i=0;i<t.size();i++)
            if(t[i]!=s2[s2.size()-1])
                s2+=t[i];
        if(s1==s2)  cout<<"Yes"<<endl;
        else    cout<<"No"<<endl;
    }
}
