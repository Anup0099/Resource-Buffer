#include <bits/stdc++.h>
using namespace std;
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
        int t;
        cin>>t;
        while(t--)
        {
          long n,m;
          cin>>n>>m;
          long x=__gcd(n,m);
          long y=(n*m)/x;
          cout<<x<<" "<<y<<"\n";
        }
	return 0;
         }
